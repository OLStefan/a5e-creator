{
  "name": "a5e-creator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "develop": "yarn install && parcel src/index.html -p 3000",
    "build": "parcel build src/index.html",
    "build:docker": "docker build . -t a5e-creator",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "check:types": "tsc --noEmit",
    "check:style": "prettier --write .",
    "check:linter": "eslint . --ext .tsx --ext .ts",
    "check:all": "yarn check:types && yarn check:style && yarn check:linter && yarn test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "dependencies": {
    "@ant-design/icons": "~4.7.0",
    "@fortawesome/fontawesome-svg-core": "~6.2.0",
    "@fortawesome/free-solid-svg-icons": "~6.2.0",
    "@reduxjs/toolkit": "~1.8.5",
    "antd": "~4.23.4",
    "classnames": "~2.3.2",
    "goober": "~2.1.11",
    "lodash": "~4.17.21",
    "myzod": "~1.8.8",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-redux": "~8.0.4",
    "react-scripts": "5.0.1",
    "redux": "~4.2.0",
    "redux-devtools-extension": "~2.13.9",
    "redux-thunk": "~2.4.1",
    "use-updating-callbacks": "~1.0.1"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.7.0",
    "@parcel/transformer-webmanifest": "2.7.0",
    "@testing-library/dom": "~8.18.1",
    "@testing-library/jest-dom": "~5.16.5",
    "@testing-library/react": "~12.1.5",
    "@testing-library/user-event": "~14.4.3",
    "@types/jest": "~29.1.1",
    "@types/lodash": "~4.14.186",
    "@types/node": "~18.7.23",
    "@types/react": "~17.0.50",
    "@types/react-dom": "~17.0.17",
    "csstype": "~3.1.1",
    "eslint": "~8.24.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jsx-a11y": "~6.6.1",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-react": "~7.31.8",
    "eslint-plugin-react-hooks": "~4.6.0",
    "jest": "~29.1.2",
    "jest-environment-jsdom": "~29.1.2",
    "parcel": "~2.7.0",
    "prettier": "~2.7.1",
    "process": "~0.11.10",
    "type-fest": "~3.0.0",
    "typescript": "~4.8.4"
  }
