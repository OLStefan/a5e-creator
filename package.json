{
	"name": "a5e-creator",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"develop": "yarn install && next dev",
		"export": "echo 'Not yet supported in next.JS' && exit 1; yarn build && next export",
		"build": "yarn parse:data-files && next build",
		"build:docker": "docker build . -t a5e-creator",
		"clean": "rm -rf out node_modules .next",
		"test": "jest",
		"test:watch": "yarn test --watch",
		"check:types": "tsc --noEmit --incremental",
		"check:style": "prettier --check ./src",
		"check:lint": "next lint",
		"check:all": "yarn check:types && yarn check:style && yarn check:lint && yarn test",
		"fix:style": "prettier --write ./src",
		"parse:data-files": "yarn test 'src/data/models/'"
	},
	"browserslist": "> 0.5%, last 2 versions, not dead",
	"dependencies": {
		"@ant-design/icons": "~5.0.1",
		"@fortawesome/fontawesome-svg-core": "~6.2.1",
		"@fortawesome/free-solid-svg-icons": "~6.2.1",
		"@next/font": "^13.1.6",
		"antd": "~5.2.0",
		"classnames": "~2.3.2",
		"lodash": "~4.17.21",
		"mobx": "^6.8.0",
		"mobx-react-lite": "^3.4.0",
		"mobx-state-tree": "^5.1.7",
		"next": "13.1.6",
		"react": "~18.2.0",
		"react-dom": "~18.2.0",
		"react-markdown": "^8.0.5",
		"react-redux": "^8.0.5",
		"use-updating-callbacks": "~1.0.1"
	},
	"devDependencies": {
		"@testing-library/dom": "~8.20.0",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/react": "13.4.0",
		"@testing-library/user-event": "14.4.3",
		"@types/jest": "~29.4.0",
		"@types/lodash": "~4.14.191",
		"@types/node": "~18.13.0",
		"@types/react": "18.0.28",
		"@types/react-dom": "~18.0.10",
		"@types/testing-library__jest-dom": "5.14.5",
		"@typescript-eslint/eslint-plugin": "~5.51.0",
		"@typescript-eslint/parser": "~5.51.0",
		"csstype": "~3.1.1",
		"eslint": "~8.34.0",
		"eslint-config-next": "13.1.6",
		"eslint-config-prettier": "~8.6.0",
		"eslint-plugin-testing-library": "^5.10.1",
		"jest": "~29.4.2",
		"jest-environment-jsdom": "~29.4.2",
		"prettier": "~2.8.4",
		"process": "~0.11.10",
		"sass": "^1.58.0",
		"ts-jest": "~29.0.5",
		"type-fest": "~3.5.7",
		"typescript": "~4.9.5",
		"typescript-plugin-css-modules": "^4.1.1"
	}
}
