{
  "name": "a5e-creator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.23.3",
    "classnames": "^2.3.2",
    "goober": "^2.1.11",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.4",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "use-updating-callbacks": "^1.0.1"
  },
  "scripts": {
    "develop": "yarn install && BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "clean": "rimraf node_modules build",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "check:types": "tsc --noEmit",
    "check:style": "prettier --check .",
    "check:linter": "eslint . --ext .tsx --ext .ts",
    "check:all": "yarn check:types && yarn check:style && yarn check:linter && yarn test",
    "prettier:fix": "prettier --write .",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "yarn lint && gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.1",
    "@types/node": "^18.7.23",
    "@types/react": "^17.0.50",
    "@types/react-dom": "^17.0.17",
    "csstype": "^3.1.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
