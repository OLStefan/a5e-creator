{
	"name": "a5e-creator",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"engines": {
		"node": "^16"
	},
	"scripts": {
		"develop": "yarn install && yarn pre:dev && next dev",
		"build": "yarn parse:data-files && yarn pre:dev && next build",
		"build:standalone": "cp next.config.js next.config.bak.js && cp next-standalone.config.js next.config.js && yarn build; mv next.config.bak.js next.config.js",
		"build:docker": "docker build . -t a5e-creator",
		"clean-server-actions": "find src/**/*.ts | xargs grep -il \"'use server';\" | xargs -r sed -i \"s/'use server';//g\"",
		"export": "cp next.config.js next.config.bak.js && cp next-static.config.js next.config.js && yarn clean-server-actions && yarn build; mv next.config.bak.js next.config.js",
		"copy-static": "cp -r .next/static .next/standalone/.next",
		"copy-public": "cp -r public .next/standalone/",
		"start": "next start",
		"start:docker": "docker run -d -p 3000:3000 a5e-creator",
		"clean": "rm -rf out node_modules .next",
		"test": "jest",
		"test:watch": "yarn test --watch",
		"check:types": "tsc --noEmit --incremental",
		"check:style": "prettier --check ./src",
		"check:lint": "next lint && stylelint src/**/*.css",
		"check:all": "yarn check:types && yarn check:style && yarn check:lint && yarn test",
		"fix:style": "prettier --write ./src",
		"parse:data-files": "yarn test src/data/models/",
		"pre:dev": "tsx ./src/scripts/genAntdCss.ts",
		"pre:build": "cross-env NODE_ENV=production tsx ./src/scripts/genAntdCss.ts"
	},
	"browserslist": "> 0.5%, last 2 versions, not dead",
	"dependencies": {
		"@ant-design/icons": "~5.2.5",
		"@ant-design/static-style-extract": "^1.0.1",
		"@fortawesome/fontawesome-svg-core": "~6.4.2",
		"@fortawesome/free-solid-svg-icons": "~6.4.2",
		"antd": "~5.8.4",
		"classnames": "~2.3.2",
		"lodash": "~4.17.21",
		"mobx": "^6.10.0",
		"mobx-react-lite": "^4.0.3",
		"mobx-state-tree": "^5.1.8",
		"next": "13.4.19",
		"react": "~18.2.0",
		"react-dom": "~18.2.0",
		"react-markdown": "^8.0.7",
		"use-updating-callbacks": "~1.1.0"
	},
	"devDependencies": {
		"@testing-library/dom": "~9.3.1",
		"@testing-library/react": "~14.0.0",
		"@testing-library/user-event": "~14.4.3",
		"@types/jest": "~29.5.4",
		"@types/lodash": "~4.14.197",
		"@types/node": "^16.18.44",
		"@types/react": "~18.2.21",
		"@types/react-dom": "~18.2.7",
		"@typescript-eslint/eslint-plugin": "~6.4.1",
		"@typescript-eslint/parser": "~6.4.1",
		"cross-env": "^7.0.3",
		"csstype": "~3.1.2",
		"eslint": "~8.47.0",
		"eslint-config-next": "~13.4.19",
		"eslint-config-prettier": "~9.0.0",
		"eslint-plugin-testing-library": "~6.0.1",
		"jest": "~29.6.4",
		"jest-environment-jsdom": "~29.6.4",
		"postcss": "^8.4.28",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-nesting": "^12.0.1",
		"postcss-plugin-namespace": "^0.0.3",
		"postcss-preset-env": "^9.1.1",
		"prettier": "~3.0.2",
		"stylelint": "^15.10.3",
		"stylelint-config-css-modules": "^4.3.0",
		"stylelint-config-standard": "^34.0.0",
		"ts-jest": "~29.1.1",
		"ts-node": "^10.9.1",
		"tslib": "^2.6.2",
		"tsx": "^3.12.7",
		"type-fest": "~4.2.0",
		"typescript": "~5.2.2",
		"typescript-plugin-css-modules": "^5.0.1"
	}
}
