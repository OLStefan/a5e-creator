{
	"name": "a5e-creator",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"develop": "yarn install && parcel src/index.html -p 3000",
		"build": "yarn parse:data-files && parcel build src/index.html --public-url .",
		"build:docker": "docker build . -t a5e-creator",
		"clean": "rm -rf dist node_modules .parcel-cache",
		"test": "jest --",
		"test:watch": "yarn test --watch",
		"check:types": "tsc --noEmit",
		"check:style": "prettier --check ./src",
		"check:lint": "eslint . --ext .tsx --ext .ts",
		"check:all": "yarn check:types && yarn check:style && yarn check:lint && yarn test",
		"fix:style": "prettier --write ./src",
		"parse:data-files": "yarn test 'src/data/schemas/'"
	},
	"browserslist": "> 0.5%, last 2 versions, not dead",
	"dependencies": {
		"@ant-design/icons": "~5.0.1",
		"@fortawesome/fontawesome-svg-core": "~6.2.1",
		"@fortawesome/free-solid-svg-icons": "~6.2.1",
		"@parcel/core": "^2.8.3",
		"@reduxjs/toolkit": "~1.9.2",
		"antd": "~5.1.7",
		"classnames": "~2.3.2",
		"goober": "~2.1.12",
		"lodash": "~4.17.21",
		"mobx": "^6.7.0",
		"mobx-react": "^7.6.0",
		"mobx-state-tree": "^5.1.7",
		"myzod": "~1.8.8",
		"react": "~18.2.0",
		"react-dom": "~18.2.0",
		"react-markdown": "^8.0.5",
		"react-redux": "^8.0.5",
		"use-updating-callbacks": "~1.0.1"
	},
	"devDependencies": {
		"@parcel/packager-raw-url": "2.8.3",
		"@parcel/transformer-webmanifest": "2.8.3",
		"@testing-library/dom": "~8.20.0",
		"@testing-library/jest-dom": "~5.16.5",
		"@testing-library/react": "~13.4.0",
		"@testing-library/user-event": "~14.4.3",
		"@types/jest": "~29.4.0",
		"@types/lodash": "~4.14.191",
		"@types/node": "~18.11.18",
		"@types/react": "~18.0.27",
		"@types/react-dom": "~18.0.10",
		"@typescript-eslint/eslint-plugin": "~5.50.0",
		"@typescript-eslint/parser": "~5.50.0",
		"csstype": "~3.1.1",
		"eslint": "~8.33.0",
		"eslint-config-prettier": "~8.6.0",
		"eslint-import-resolver-typescript": "^3.5.3",
		"eslint-plugin-import": "~2.27.5",
		"eslint-plugin-jsx-a11y": "~6.7.1",
		"eslint-plugin-prettier": "~4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"jest": "~29.4.1",
		"jest-environment-jsdom": "~29.4.1",
		"parcel": "~2.8.3",
		"prettier": "~2.8.3",
		"process": "~0.11.10",
		"ts-jest": "~29.0.5",
		"type-fest": "~3.5.4",
		"typescript": "~4.9.5"
	}
}
